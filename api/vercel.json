{
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/payment" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" },
        { "key": "Access-Control-Allow-Methods", "value": "GET,POST" }
      ]
    }
  ]
}
